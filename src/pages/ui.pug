extends ../templates/template.pug
block content
  .container.u-center.u-flex-col
      h1 Header 1
      h2 Header 2
      h3 Header 3
      h4 Header 4
      h5 Header 5
      br
      
      h1.t-h1 Header 1
      h2.t-h2 Header 2
      h3.t-h3 Header 3
      h4.t-h4 Header 4
      h5.t-h5 Header 5
      br

      a(href="") Ссылка
      button.t-ttu Uppercase

      br

      +btn('@peshehod.spb').btn--light
      +btn('Купить билет')
      +btn('Купить билет').btn--pulse
      +btn('Купить билет').btn--pulse.btn--dark
      +btn('Выбрать тур', true)

      br
      +social
      
      br
      div(style="width: 375px;")
        +slider-nav

      br
      .select-box
        +svg('chevron')
        select
          option(value="val-1") 5 декабря 2019 в 15:30
          option(value="val-2") 31 декабря 2019 в 00:00
          option(value="val-3") 5 января 2020 в 10:15

      br
      .select-text
        +svg('chevron')
        select
          option(value="val-1") Все маршруты
          option(value="val-2") Василеостровская
          option(selected value="val-3") Центральные каналы


